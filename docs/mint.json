{
  "$schema": "https://mintlify.com/schema.json",
  "name": "Station",
  "logo": {
    "dark": "/logo/dark.svg",
    "light": "/logo/light.svg"
  },
  "favicon": "/favicon.png",
  "colors": {
    "primary": "#AD72FF",
    "light": "#B889FA",
    "dark": "#8B38FF",
    "background": {
      "dark": "#000"
    },
    "anchors": {
      "from": "#8B38FF",
      "to": "#AD72FF"
    }
  },
  "modeToggle": {
    "default": "dark",
    "isHidden": true
